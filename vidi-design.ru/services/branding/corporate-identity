ctorAll('.' + mainclass + ` [class*="vcatbuy"]`).forEach(function(element) { element.addEventListener('click', function(e) { let cl = getnum(element.className, "vcatbuy") + 1; let quant = document.querySelector('.' + mainclass + ' .vcatquant' + cl + ' input'); if (quant) quant.value = 1; }); }); document.querySelectorAll('.' + mainclass + ' [class*="vcatquant"]').forEach(function(element) { if (element) { let submitButton = element.querySelector('.tn-form__submit'); if (submitButton) { submitButton.remove(); } element.addEventListener('input', function(e) { let cl = getnum(element.className, "vcatquant") + 1; let buyElement = document.querySelector('.' + mainclass + ' .vcatbuy' + cl); if (e.target.value < 1) e.target.value = 1; else if (buyElement && parseFloat(e.target.value) > parseFloat(buyElement.getAttribute('data-product-inv'))) { let maxInput = buyElement.querySelector('.t-store__prod__quantity input'); if (maxInput) { e.target.value = maxInput.max; } } else if (buyElement) { let quantityInput = buyElement.querySelector('.t-store__prod__quantity input'); if (quantityInput) { quantityInput.value = e.target.value || 1; } } }); } }); document.querySelectorAll('.' + mainclass + ' [class*="vcatvar"]').forEach(function(element) { element.addEventListener('change', function(e) { let cl = getnum(element.className, "vcatvar") + 1; let id = parseFloat(document.querySelector('.' + mainclass + ' .vcatbuy' + cl).getAttribute('data-product-uid')); document.querySelector('.' + mainclass + ' .vcatbuy' + cl + ' select [value="' + e.target.value + '"]').selected = true; voronUpdateOne(mainclass, voroncatproducts[mainclass].products.find(item => item.editions.some(edition => edition.uid === id) ), cl); }); }); }; window.addEventListener("load", function () { voroncatinit('uc-set1', Array.from(document.querySelectorAll('.uc-set1 [class*="catname"]')).filter(isElementInArtboard).length, '', 233086059572); }) </script> <!-- nominify end --> </div> </div> </div> </div> <!--footer--